#!/usr/bin/env ruby

require 'bundler/setup'
require 'KeggyFuzzer'

@hammer = Hammer::Parser
parser = @hammer.sequence(@hammer.token("defg"),
                          @hammer.many1(@hammer.ch_range('c', 'z')),
                          @hammer.end_p)
fuzz = KeggyFuzzer::KeggyFuzzer.new(parser, 10)
fuzz.test_parsers
